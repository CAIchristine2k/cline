  "graphql_image_sources": {
    "description": "Les images de produits Shopify sont chargées dynamiquement via ces queries GraphQL",
    "queries": [
      {
        "file": "app/routes/($locale)._index.tsx",
        "query_name": "PRODUCTS_QUERY",
        "line_start": 165,
        "image_fields": [
          "featuredImage { id, url, altText, width, height }",
          "images(first: 1) { nodes { url, altText, width, height } }",
          "variants(first: 25) { nodes { image { url, altText, width, height } } }"
        ]
      },
      {
        "file": "app/routes/($locale)._index.tsx",
        "query_name": "BESTSELLERS_COLLECTION_QUERY",
        "line_start": 249,
        "image_fields": [
          "collection.products.nodes.featuredImage { id, url, altText, width, height }",
          "collection.products.nodes.variants.nodes.image (non inclus actuellement)"
        ]
      },
      {
        "file": "app/routes/($locale).products.$handle.tsx",
        "query_name": "PRODUCT_QUERY",
        "line_start": 993,
        "image_fields": [
          "product.featuredImage { id, url, altText, width, height }",
          "product.images(first: 10) { nodes { id, url, altText, width, height } }",
          "product.variants.nodes.image { id, url, altText, width, height }"
        ]
      },
      {
        "file": "app/routes/($locale).collections.$handle.tsx",
        "query_name": "COLLECTION_QUERY",
        "image_fields": [
          "collection.image { url, altText, width, height }",
          "collection.products.nodes.featuredImage { id, url, altText, width, height }"
        ]
      }
    ]
  },
  "how_to_recover_shopify_images": {
    "method_1": {
      "name": "Shopify Admin - Products & Files",
      "steps": [
        "Connectez-vous à votre Shopify Admin",
        "Allez dans Products → Tous les produits (y compris archivés)",
        "Pour chaque produit, clic droit sur l'image → Copier l'adresse de l'image",
        "Ou allez dans Settings → Files pour voir tous les fichiers uploadés"
      ],
      "note": "Les images de produits supprimés restent accessibles pendant 60-90 jours"
    },
    "method_2": {
      "name": "Browser DevTools - Network Tab",
      "steps": [
        "Lancez 'npm run dev' dans votre terminal",
        "Ouvrez votre navigateur à localhost:3000",
        "Ouvrez DevTools (F12) → Onglet Network",
        "Filtrez par 'Img' ou cherchez 'cdn.shopify.com'",
        "Naviguez sur votre site pour charger les images",
        "Copiez les URLs des images chargées"
      ]
    },
    "method_3": {
      "name": "GraphQL API Directe",
      "description": "Utilisez l'API Shopify Storefront pour récupérer toutes les URLs d'images",
      "example_query": "{ products(first: 250) { nodes { id title images(first: 10) { nodes { url originalSrc transformedSrc } } } } }"
    },
    "method_4": {
      "name": "Export CSV des produits",
      "steps": [
        "Shopify Admin → Products → Export",
        "Sélectionnez 'Tous les produits' ou 'Produits sélectionnés'",
        "Le CSV contiendra les colonnes 'Image Src' avec les URLs"
      ]
    },
    "method_5": {
      "name": "Shopify Files Archive",
      "description": "Settings → Files → Tous les fichiers uploadés sur votre boutique sont listés ici avec leurs URLs CDN"
    }
  },
